        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/feedback",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.7.8",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\Kustomer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\Kustomer\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\Kustomer\\KustomerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
